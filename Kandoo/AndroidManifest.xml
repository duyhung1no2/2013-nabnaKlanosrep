<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duyhungapp.kandoo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.duyhungapp.kandoo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.duyhungapp.kandoo.activity.TaskAddActivity"
            android:label="@string/title_activity_task_add" >
        </activity>
        <activity
            android:name="com.duyhungapp.kandoo.activity.TaskDetailActivity"
            android:label="@string/title_activity_task_detail" >
        </activity>
        <activity
            android:name="com.duyhungapp.kandoo.activity.TaskEditActivity"
            android:label="@string/title_activity_task_edit" >
        </activity>
        <activity
            android:name="com.duyhungapp.kandoo.activity.BoardManageActivity"
            android:label="@string/title_activity_board_manage" >
        </activity>
    </application>

</manifest>